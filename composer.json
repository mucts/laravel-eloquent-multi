{
    "name": "mucts/laravel-eloquent-multi",
    "description": "Laravel Eloquent For Multi Query",
    "type": "library",
    "require": {
        "php": ">=7.2",
        "ext-bcmath": "*",
        "ext-gmp": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "laravel/framework": "^7.0",
        "grimzy/laravel-mysql-spatial": "^2.1",
        "mucts/helpers": "^1.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.6",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "authors": [
        {
            "name": "herry",
            "email": "yuandeng@aliyun.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "MuCTS\\Laravel\\EloquentMulti\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MuCTS\\Laravel\\EloquentMulti\\Providers\\ServiceProvider"
            ]
        }
    }
}
